{
  "name": "friday",
  "version": "3.0.0",
  "private": true,
  "engines": {
    "node": ">=16.18.1 <17",
    "yarn": "1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.ts": "eslint",
    "*.tsx": "eslint",
    "*.json": "prettier --write",
    "*.{md,mdx,markdown}": "prettier --write",
    "*": "yarn commitlint -E HUSKY_GIT_PARAMS"
  },
  "dependencies": {
    "@types/node": "18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "next": "13.2.3",
    "next-i18next": "^13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "styled-components": "^5.3.6",
    "styled-components-ts": "^0.0.15",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/styled-components": "^5.1.26",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2"
  }
}
